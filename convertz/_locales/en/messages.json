   "descr_colour": {
      "message": "\u003Cstrong>Select the colour with which to highlight the converted numbers and units.\u003C/strong> \u003Cbr/>\u003Cbr/>Choose 'None' to disable highlighting (not recommended)."
   },
   "descr_currency": {
      "message": "\u003Cstrong>Select your local currency unit.\u003C/strong>\u003Cbr/>Amounts in other currencies will be converted to your home currency."
   },
   "descr_extras": {
      "message": "\u003Cstrong>\u003Ci class='icon-exclamation-sign'>\u003C/i> Advanced options. Please do not change unless you are sure of the effects.\u003C/strong>"
   },
   "descr_feedback": {
      "message": ""
   },
   "descr_nr": {
      "message": "\u003Cstrong>Decide whether to keep original values in web page or replace them with converted values.\u003C/strong>\u003Cbr/>\u003Cbr/>Default behaviour is to keep original values and show converted values in tool tip."
   },
   "descr_timeZone": {
      "message": "\u003Cstrong>\u003Ci class='icon-exclamation-sign'>\u003C/i>Pro feature, requires in-app payment after initial trial.\u003Cbr/>\u003Cbr/>Select your local, and additional time zones.\u003C/strong>\u003Cbr/>Times listed in other time zones will be converted to your home time zone.\u003Cbr>Additional time zones specified will be available on hover, e.g time zones of all your other team members."
   },
   "descr_us": {
      "message": "\u003Cstrong>Select the unit system that you use.\u003C/strong> \u003Cbr/>\u003Cbr/>Values in other units will be converted to units in your system.\u003Cbr/>\u003Cbr/>You can also select a custom mix of units by selecting different units for each value type below."
   },
   "descr_whitelist": {
      "message": "\u003Cstrong>Whitelist websites and phrases to stop auto-conversion.\u003C/strong>\u003Cbr/>\u003Cbr/>Select websites on which you want to disable auto-conversion. Also, select any specific phrases you want to prevent from converting (e.g. PHP5)."
   },
